{
  "name": "clovers/clover",
  "keywords": ["cms", "php", "PHP Framework" ],
  "description": "Clover - 为速度而生的 PHP 框架",
  "version": "2.0.0-dev",
  "type": "project",
  "homepage": "http://doc.mengniang.tv/clover/1268895",
  "license": "MIT",
  "authors": [
    {
      "name": "Donghai Chen",
      "email": "chendonghai888@gmail.com",
      "homepage": "https://blog.mengniang.tv",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "noahbuscher/macaw": "dev-master",
    "filp/whoops": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "start": "cd clover && php -S 127.0.0.1:8888 -t public"
  }
}
