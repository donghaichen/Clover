{
    "name": "theseus/theseus",
    "description": "The Theseus Framework.",
    "keywords": ["framework", "theseus", "php", "API", "RESTful", "API框架", "PHP框架"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "illuminate/database": "~5.2",
        "overtrue/validation": "1.0",
        "monolog/monolog": "1.13.1",
        "slim/slim": "~3.0"
    },
    "autoload": {
        "classmap": [
            "app"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
